{
  "expo": {
    "name": "questnik45",
    "slug": "questnik45",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logoq45.png",
    "scheme": "questnik45",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logoq45.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.klaudia.prucz.questnik45"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/favicon (2).png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/logoq45.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "799f7b4a-6445-4109-aec6-4d611e5c3e09"
      }
    },
    "owner": "klaudia.prucz"
  }
}
